digraph finite_state_machine {
    node [label="", shape=circle];

    ss[style="invis"];

    start[shape="doublecircle"];
    q1; q2[shape="doublecircle"]; q3;
    
    start -> q1[label="0"];
    start -> q2[label="1"];
    q1 -> q1[label="0,1"];
    q2 -> q3[label="0,1"];
    q3 -> q2[label="1"];
    q3 -> q1[label="0"];

    rankdir=LR;
}

